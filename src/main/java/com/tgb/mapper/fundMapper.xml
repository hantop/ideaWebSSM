<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
	namespace：必须与对应的接口全类名一致
	id:必须与对应接口的某个对应的方法名一致

 -->
<mapper namespace="com.tgb.mapper.FundMapper">


    <select id="findAll" resultType="MyHasFundInfo">
      SELECT fund_code fund_has_code,fund_name fund_name,fund_buy_price fund_buy_price,fund_has_money fund_has_money FROM t_fund
    </select>
    <select id="findByCode" resultType="MyHasFundInfo" parameterType="String">
      SELECT fund_code fund_has_code,fund_name fund_name,fund_buy_price fund_buy_price,fund_has_money fund_has_money FROM t_fund where fund_code=#{fund_has_code}
    </select>

    <update id="updateMyFund" parameterType="MyHasFundInfo" >
        update t_fund set fund_buy_price=#{fund_buy_price},fund_has_money=#{fund_has_money} where fund_code=#{fund_has_code}
    </update>


    <insert id="saveMyFund" parameterType="MyHasFundInfo">
            insert into t_fund(fund_code,fund_name,fund_buy_price,fund_has_money)  values(#{fund_has_code},#{fund_name},#{fund_buy_price},#{fund_has_money})
    </insert>

</mapper>